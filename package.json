{
  "name": "appoutlet",
  "version": "1.0.1",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "start": "ng serve",
    "build": "ng build --localize",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:appoutlet": "node dist/appoutlet/server/server.mjs",
    "translate:extract": "ng extract-i18n --format=json --output-path src/locale ",
    "translate:replace-en": "rm src/locale/messages.en.json && cp src/locale/messages.json src/locale/messages.en.json",
    "translate:process": "npx lingo.dev@latest i18n -y",
    "translate": "npm run translate:extract && npm run translate:replace-en && npm run translate:process"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.2",
    "@angular/compiler": "^20.3.2",
    "@angular/core": "^20.3.2",
    "@angular/forms": "^20.3.2",
    "@angular/platform-browser": "^20.3.2",
    "@angular/platform-server": "^20.3.2",
    "@angular/router": "^20.3.2",
    "@angular/ssr": "^20.3.3",
    "@phosphor-icons/web": "^2.1.2",
    "@tailwindcss/postcss": "^4.1.13",
    "express": "^5.1.0",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.13",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.3",
    "@angular/cli": "^20.3.3",
    "@angular/compiler-cli": "^20.3.2",
    "@angular/localize": "^20.3.2",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.0",
    "typescript": "~5.9.3"
  },
  "packageManager": "pnpm@10.16.1+sha512.0e155aa2629db8672b49e8475da6226aa4bdea85fdcdfdc15350874946d4f3c91faaf64cbdc4a5d1ab8002f473d5c3fcedcd197989cf0390f9badd3c04678706"
}
